{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "module": "ES6",
    "lib": ["DOM", "ES2022"],
    "moduleResolution": "Node",
    "rootDir": ".",
    "target": "ES5",
    "outDir": "./build",
    "strict": true,
    "esModuleInterop": true,
    "inlineSources": true
  },
  "include": [
    "src/**/*.ts",
    "test/**/*.ts"
  ]
}
