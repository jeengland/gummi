{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "module": "system",
    "lib": ["DOM", "ES2022"],
    "rootDir": ".",
    "target": "ES5",
    "outDir": "./build",
    "outFile": "./build/index.js",
    "strict": true,
    "esModuleInterop": true,
    "inlineSources": true
  },
  "include": [
    "src//client/**/*.ts",
    "test/**/*.ts"
  ]
}
